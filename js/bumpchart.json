{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": "World Cup 2018: Group F Rankings",
      "frame": "bounds"
    },
    "data": {
      "url": "https://raw.githubusercontent.com/2311ajay/FIT3179-Vega-Lite/main/data/world-happiness-report-compiled.csv"
    },
    "transform": [{
      "sort": [
        {"field": "Life Ladder", "order": "descending"}
      ],
      "window": [{
        "op": "rank",
        "as": "rank"
      }],
      "groupby": ["year"]
    }],
    "mark": {"type": "line", "orient": "vertical"},
    "encoding": {
      "x": {"field": "year", "type": "ordinal"},
      "y": {"field": "rank", "type": "ordinal"},
      "color": {
        "field": "Country", "type": "nominal",
        "scale": {
          "domain": ["Germany", "Mexico", "South Korea", "Sweden"],
          "range": ["black", "#127153", "#C91A3C", "#0C71AB"]
        }
      }
    }
  }
  